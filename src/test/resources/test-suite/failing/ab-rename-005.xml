<t:test xmlns:t="http://xproc.org/ns/testsuite/3.0" expected="pass">
   <t:info>
      <t:title>p:rename-005 (AB)</t:title>
      <t:revision-history>
         <t:revision>
            <t:date>2019-05-18</t:date>
            <t:author>
               <t:name>Achim Berndzen</t:name>
            </t:author>
            <t:description xmlns="http://www.w3.org/1999/xhtml">
               <p>Ported tests for p:rename</p>
            </t:description>
         </t:revision>
      </t:revision-history>
   </t:info>
   <t:description xmlns="http://www.w3.org/1999/xhtml">
      <p>Tests p:rename</p>
   </t:description>
   <t:input port='source'>
      <document>
         <element/>
      </document>
   </t:input>
   <t:pipeline>
      <p:declare-step xmlns:p="http://www.w3.org/ns/xproc" version="3.0">
         <p:input port="source" />
         <p:output port="result"/>
            
         <p:rename match="element" new-name="Q{{http://baz.com}}foo" />
         
         
      </p:declare-step>
   </t:pipeline>
   <t:schematron>
      <s:schema xmlns:s="http://purl.oclc.org/dsdl/schematron" xmlns="http://www.w3.org/1999/xhtml">
         <s:ns uri="http://baz.com" prefix="baz" />
         <s:pattern>
            <s:rule context="/">
               <s:assert test="document">The root element is not 'document'.</s:assert>
               <s:assert test="document/baz:foo">Root element does not have a child Q{http://baz.com}foo</s:assert>
            </s:rule>
         </s:pattern>
      </s:schema>
   </t:schematron>
</t:test>